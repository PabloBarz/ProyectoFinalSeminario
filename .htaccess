# Configuraciones de servidor
RewriteEngine On 
Options All -Indexes

# Suprimir la extension (.php) en la navegacion URL AMIGABLES
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php [L]

# Directivas de acceso 
ErrorDocument 401 /projectofinalseminario/samples/error-500.html
ErrorDocument 400 /projectofinalseminario/samples/error-500.html
ErrorDocument 403 /projectofinalseminario/samples/error-500.html
ErrorDocument 404 /projectofinalseminario/samples/error-404.html